-- Variable declarations
x :: 10
y :: 5

-- Function definition
#add(a, b) {
    result :: a + b
    return result
}

z :: add(x, y)
>> "The sum of x and y is: " + z

-- Class definition
##Person {
    name :: ""
    age :: 0

    #init(n, a) {
        name = n
        age = a
    }

    #greet() {
        >> "Hello, my name is " + name + " and I am " + age + " years old."
    }
}

-- Create an object of Person
p :: Person()
p.init("Alice", 30)
p.greet()
